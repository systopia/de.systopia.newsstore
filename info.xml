<?xml version="1.0"?>
<extension key="de.systopia.newsstore" type="module">
  <file>newsstore</file>
  <name>NewsStore</name>
  <description>Provides basic services for news sources like RSS</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.newsstore</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.newsstore</url>
    <url desc="Support">https://github.com/systopia/de.systopia.newsstore/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2018-01-15</releaseDate>
  <version>0.2.3</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <comments>Written by Rich Lott / Artful Robot on behalf of Systopia.</comments>
  <civix>
    <namespace>CRM/Newsstore</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>ang-php@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Newsstore_Upgrader</upgrader>
</extension>
